{
    "name": "erick/todo",
    "version": "1.0.0",
    "description": "um todo com php usando bibliotecas como auxilio",
    "require": {
        "monolog/monolog": "^3.8",
        "catfan/medoo": "^2.1",
        "vlucas/phpdotenv": "^5.6",
        "relay/relay": "^3.0",
        "firebase/php-jwt": "^6.11",
        "flightphp/core": "^3.13"
    },
    "autoload": {
        "psr-4": {
            "Erick\\Todo\\": "src/",
            "Tests\\":"tests/"
        }
    },
    "authors": [
        {
            "name": "ErickVerissimoo",
            "email": "erickverissimodasilva144@gmail.com"
        }
    ],
    "require-dev": {
        "pestphp/pest": "^3.7"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
